<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.CsreplMapper">
	<update id="updateCresqPlusOne">
		<![CDATA[
			Update csrepl SET
			cresq = cresq + 1
			where crerf = #{crerf}
			AND cresq > #{cresq} 
		]]>
	</update>
</mapper>	